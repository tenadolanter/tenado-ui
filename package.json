{
  "name": "@tenado/ui",
  "version": "1.0.0",
  "description": "A Component Library for Vue.js.",
  "main": "lib/index.js",
  "files": [
    "lib",
    "src",
    "packages",
    "types"
  ],
  "typings": "types/index.d.ts",
  "scripts": {
    "dev": "npm run build:file && cross-env NODE_ENV=development webpack-dev-server --config build/webpack.demo.js",
    "clean": "rimraf lib && rimraf packages/*/lib",
    "build:file": "node build/bin/iconInit.js & node build/bin/build-entry.js",
    "build:theme": "node build/bin/gen-cssfile && gulp build --gulpfile theme-chalk/gulpfile.js && cp-cli theme-chalk/lib lib/theme-chalk",
    "build:utils": "cross-env BABEL_ENV=utils babel src --out-dir lib --ignore src/index.js",
    "build:docs": "npm run build:file && npm run build:theme && cross-env NODE_ENV=production webpack --config build/webpack.demo.js",
    "build:lib": "npm run clean && npm run build:file && webpack --config build/webpack.conf.js && webpack --config build/webpack.component.js && npm run build:utils && npm run build:theme"
  },
  "keywords": [
    "vue",
    "components"
  ],
  "license": "MIT",
  "unpkg": "lib/index.js",
  "style": "lib/theme-chalk/index.css",
  "dependencies": {
    "async-validator": "~1.8.1",
    "deepmerge": "^1.2.0",
    "normalize-wheel": "^1.0.1",
    "resize-observer-polyfill": "^1.5.0",
    "throttle-debounce": "^1.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.8",
    "@tenado/icons": "^1.0.10",
    "@vue/babel-preset-app": "^5.0.8",
    "@vue/component-compiler-utils": "^2.6.0",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^9.1.3",
    "babel-plugin-module-resolver": "^5.0.2",
    "copy-webpack-plugin": "^5.0.0",
    "core-js": "^3.37.1",
    "cp-cli": "^1.0.2",
    "cross-env": "^3.1.3",
    "css-loader": "^5.2.7",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "es6-promise": "^4.0.5",
    "file-loader": "^6.2.0",
    "file-save": "^0.2.0",
    "gulp": "^4.0.0",
    "gulp-autoprefixer": "^6.0.0",
    "gulp-cssmin": "^0.2.0",
    "gulp-sass": "^5.0.0",
    "highlight.js": "^11.10.0",
    "html-webpack-plugin": "^5.6.0",
    "json-loader": "^0.5.7",
    "json-templater": "^1.0.4",
    "launch-editor-middleware": "^2.3.0",
    "markdown-it": "^8.4.1",
    "markdown-it-anchor": "^5.0.2",
    "markdown-it-chain": "^1.3.0",
    "markdown-it-container": "^2.0.0",
    "mini-css-extract-plugin": "^2.9.0",
    "postcss": "^7.0.14",
    "rimraf": "^2.5.4",
    "sass": "^1.77.8",
    "sass-loader": "^10.5.2",
    "style-loader": "^3.3.4",
    "terser-webpack-plugin": "^5.3.10",
    "transliteration": "^1.1.11",
    "uppercamelcase": "^1.1.0",
    "vue": "2.6.14",
    "vue-loader": "^15.7.0",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "2.6.14",
    "vue-template-es2015-compiler": "^1.6.0",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.2",
    "webpack-node-externals": "^1.7.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
